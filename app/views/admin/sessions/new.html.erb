<h2>Log in</h2>

  <%= form_with model: @admin, url: admin_session_path, local: true do |f| %>
    <table>
      <tr>
        <th><%= f.label :メールアドレス %></th>
        <td><%= f.email_field :email, autofocus: true %></td>
      </tr>

        <tr>
        <th><%= f.label :パスワード %></th>
        <td><%= f.password_field :password %></td>
      </tr>
      <tr>
        <td><%= f.submit "ログイン", class: "btn btn-primary" %></td>
      </tr>
  </table>
<% end %>

<%= render "admin/shared/links" %>
